}

layer {
  name: "argmax_l3"
  type: "ArgMax"
  bottom: "label_move3_f"
  top: "label_move3_m"
}

layer {
  name: "loss"
  type: "SoftmaxWithLoss"
  # input
  bottom: "flatten_o1"
  bottom: "label_move1"
  # output
  top: "loss"
  # loss_weight: 1.0
}

layer {
  name: "loss_2"
  type: "SoftmaxWithLoss"
  # input
  bottom: "flatten_o2"
  bottom: "label_move2_m"
  # output
  top: "loss_2"
  # loss_weight: 0.3
}

layer {
  name: "loss_3"
