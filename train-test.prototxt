  # input
  bottom: "flatten_o3"
  bottom: "label_move3_m"
  # output
  top: "loss_3"
  # loss_weight: 0.2
}

layer {
  name: "accuracy"
  type: "Accuracy"
  # input
  bottom: "flatten_o1"
  bottom: "label_move1"
  include {
    phase: TEST
  }
  # output
  top: "accuracy"
}

layer {
  name: "accuracy2"
  type: "Accuracy"
  # input
  bottom: "flatten_o2"
  bottom: "label_move2_m"
  include {
    phase: TEST
  }
  # output
