  name: "flatten_o1"
  type: "Flatten"
  bottom: "conv14_o1"
  top: "flatten_o1"
}

layer {
  name: "flatten_o2"
  type: "Flatten"
  bottom: "conv14_o2"
  top: "flatten_o2"
