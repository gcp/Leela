  # input -> output
  bottom: "conv13"
  top: "conv14"
  param { lr_mult: 1 decay_mult: 1 }
  param { lr_mult: 2 decay_mult: 0 }
  convolution_param {
    num_output: 3
    kernel_size: 3
    pad: 1
