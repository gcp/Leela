layer {
  name: "flatten_l2"
  type: "Flatten"
  bottom: "label_move2"
  top: "label_move2_f"
  flatten_param {
    axis: 1
  }
}

layer {
  name: "argmax_l2"
  type: "ArgMax"
  bottom: "label_move2_f"
  top: "label_move2_m"
}

layer {
  name: "flatten_l3"
  type: "Flatten"
